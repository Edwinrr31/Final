@model IEnumerable<Final.Models.Stock>

@{
    ViewBag.Title = "Facturaciones";
}

<h2>Consulta de clientes por Nombre y Categoria</h2>

@using (Html.BeginForm("Facturaciones", "Consulta", FormMethod.Get))
{
    @Html.TextBox("searching")
    <input Type="submit" value="submit" />
}

<table>
    <tr>
        <th> Fecha </th>
        <th> Cliente </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td> @Html.DisplayFor(modelItem => item.Nombre)</td>
            <td> @Html.DisplayFor(modelItem => item.Proveedor)</td>
        </tr>
    }

</table>